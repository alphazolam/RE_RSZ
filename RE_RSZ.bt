        finished = false;
        if (level == 1 && !FEof()) { 
            if (Header.magic == 4343376 || Header.magic == 5129043) {
                FSkip(-1); BLANK blank; 
                FSkip(-1); BLANK userDataNotice <name=ReadUserDataNotice>; 
            }
        }
        if (ReadUInt(FTell()+8) == 0)
