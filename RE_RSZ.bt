    void WriteNewResourceInfo(ubyte &d, wstring s) {
        local uint insertPt = startof(ResourceInfos) + sizeof(ResourceInfos);
        local uint sizeToAdd = sizeof(s)-2;
        local uint padBytes;
        if (exists(ResourceInfos.ResourceInfo.pathStr.strOffset)) { //SCN
            ReadInt64(insertPt) != 0 ? sizeToAdd = 16 : sizeToAdd = 0;
            FixOffsets(0, startof(GameObjectInfos), startof(ResourceInfos)+sizeof(ResourceInfos), sizeToAdd, 0);
            if (exists(PrefabInfos))
                FixOffsets(startof(ResourceInfos), startof(PrefabInfos)+sizeof(PrefabInfos), startof(ResourceInfos)+sizeof(ResourceInfos), sizeToAdd, 0);
            else
                FixOffsets(startof(ResourceInfos), startof(ResourceInfos)+sizeof(ResourceInfos), startof(ResourceInfos)+sizeof(ResourceInfos), sizeToAdd, 0);
            if (sizeToAdd)
                InsertBytes(insertPt, sizeToAdd, 0);
            WriteInt64(insertPt, FindFirst(s, 1, 0, 0, 0, 1, RSZOffset+16));
            for (j=0; j<Header.resourceCount; j++)
                WriteInt64(startof(ResourceInfos.ResourceInfo[j]), FindFirst(ResourceInfos.ResourceInfo[j].pathStr.String));
        } else {
            while ((insertPt + sizeToAdd + padBytes) % 16 != insertPt % 16 )
                padBytes++;
            sizeToAdd += padBytes;
            FixOffsets(0, startof(GameObjectInfos), startof(ResourceInfos)+sizeof(ResourceInfos), sizeToAdd, 0);
            InsertBytes(insertPt, sizeToAdd, 0);
            WriteWString(insertPt, s);
        }
        Header.resourceCount += 1;
        ShowRefreshMessage();
    }

