        string ReadGameObjectString(ubyte &dd) {
            local ushort ii = RSZFile[0].ObjectTable[parentof(dd).id.id];
            local ushort compCtr;
            local string str;
            while(exists(RSZFile[0].InstanceInfos.instanceInfo[ii]) && compCtr < parentof(dd).componentCount + 1) {
                if (RSZFile[0].InstanceInfos.instanceInfo[ii].isObject) {
                    SPrintf(str, "%s *%i_%i", str, RSZFile[0].InstanceInfos.instanceInfo[ii].typeId.Hash, RSZFile[0].InstanceInfos.instanceInfo[ii].CRC);
                    compCtr++;
                } else
                    SPrintf(str, "%s %i_%i", str, RSZFile[0].InstanceInfos.instanceInfo[ii].typeId.Hash, RSZFile[0].InstanceInfos.instanceInfo[ii].CRC);
                ii++;
            }
            return str;
        }

        void InstanceWriterMethod(string s, ushort lvl, short insertIndex, ubyte structType) {
            local short i, j, ctr, padBytes, newId;
            local string tmp = s;
            local short Arr[(sizeof(s)/8) + 2];
            local short gObjectSz;

            if (structType == 2) //FolderInfo
                gObjectSz = 8;
            if (structType == 1) //GameObject
                Header.magic == 4343376 ? gObjectSz = 12 : gObjectSz = 32;
            
            local short sizeToAdd = gObjectSz;
            local short objectCtr;
            newId = RSZFile[lvl].RSZHeader.objectCount;
            
            local short index = findS(s, " ");
            while(index > -1) {
                Arr[ctr] = index + 1 + ctr;
                if (s[Arr[ctr]] == 0x2A) {
                    sizeToAdd+=12;
                    objectCtr++;
                } else 
                    sizeToAdd+=8;
                tmp = StrDel(tmp, index, 1);
                index = findS(tmp, " ");
                ctr++;
            }
        
            //calculate padding
            local uint padPt = (startof(RSZFile[lvl].InstanceInfos) + sizeof(RSZFile[lvl].InstanceInfos));
            local uint existingPadding;
            while(ReadByte(padPt + existingPadding) == 0) 
                existingPadding++;
            while((padPt + sizeToAdd + padBytes ) % 16 != (padPt) % 16)
                padBytes++;
            if (existingPadding + padBytes > 16)
                padBytes -= 16;
            sizeToAdd += padBytes;
            //Printf("%s %i %i %i %i %i %i ", s, structType, lvl, gObjectSz, padBytes, existingPadding, sizeToAdd);
            
            if (structType == 1)
                FixOffsets(0, RSZOffset, startof(GameObjectInfos) + sizeof(GameObjectInfos), gObjectSz, 0);
            if (structType == 2)
                FixOffsets(0, RSZOffset, startof(FolderInfos) + sizeof(FolderInfos), gObjectSz, 0);
            RSZFile[lvl].RSZHeader.instanceOffset = 48 + RSZFile[lvl].RSZHeader.objectCount * 4;
            RSZFile[lvl].RSZHeader.dataOffset += sizeToAdd - gObjectSz;
            RSZFile[lvl].RSZHeader.userdataOffset += sizeToAdd - gObjectSz;
            
            if (exists(RSZFile[lvl].RSZUserDataInfos))
                FixOffsets(startof(RSZFile[lvl].RSZUserDataInfos), startof(RSZFile[lvl].RSZUserDataInfos)+sizeof(RSZFile[lvl].RSZUserDataInfos), 
                    0, sizeToAdd - gObjectSz, 0);
            
            
            if (startof(RSZFile[lvl].InstanceInfos) + (8 * insertIndex) < padPt) { //
                //correct ObjectTable for when an instance is inserted in the middle:
                for (o=0; o<RSZFile[lvl].RSZHeader.objectCount-1; o++) 
                    if (RSZFile[lvl].ObjectTable[o] > insertIndex)
                        RSZFile[lvl].ObjectTable[o] += ctr;
            }

            if (padBytes > 0)
                InsertBytes(padPt, padBytes, 0);
            if (padBytes < 0)
                DeleteBytes(padPt, Abs(padBytes));
            
            for (i=0; i<ctr; i++) {
                InsertBytes(startof(RSZFile[lvl].InstanceInfos) + (insertIndex * 8) + 8*i, 8, 0);
                tmp = SubStr(s, Arr[i], Arr[i+1] - Arr[i]);
                if (tmp[0] == 0x2A)
                    tmp = StrDel(tmp, 0, 1);
                WriteUInt(startof(RSZFile[lvl].InstanceInfos) + (insertIndex * 8) + 8*i, Atoi(tmp)); //Hash
                WriteUInt(4 + startof(RSZFile[lvl].InstanceInfos) + (insertIndex * 8) + 8*i, Atoi(StrDel(tmp, 0, findS(tmp, "_")+1))); //CRC
            }
            
            objectCtr = 0;
            for (i=0; i<ctr; i++) {
                if (s[Arr[i]] == 0x2A) {
                    InsertBytes(startof(RSZFile[lvl].ObjectTable) + sizeof(RSZFile[lvl].ObjectTable) + 4*objectCtr, 4, 0);
                    WriteUInt(startof(RSZFile[lvl].ObjectTable) + sizeof(RSZFile[lvl].ObjectTable) + 4*objectCtr, RSZFile[lvl].RSZHeader.instanceCount + i);
                    objectCtr++;
                }        
            }
            
            RSZFile[lvl].RSZHeader.instanceOffset += objectCtr*4;
            RSZFile[lvl].RSZHeader.objectCount += objectCtr;
            RSZFile[lvl].RSZHeader.instanceCount += ctr;

            if (structType == 1) {
                local char buffer[gObjectSz];
                ReadBytes(buffer, startof(GameObjectInfos)+sizeof(GameObjectInfos)-gObjectSz, gObjectSz);
                InsertBytes(startof(GameObjectInfos)+sizeof(GameObjectInfos), gObjectSz, 0);
                WriteBytes(buffer, startof(GameObjectInfos)+sizeof(GameObjectInfos), gObjectSz);
                if (Header.magic == 4343376) { //PFB
                    WriteInt(startof(GameObjectInfos)+sizeof(GameObjectInfos), newId);
                    WriteInt(startof(GameObjectInfos)+sizeof(GameObjectInfos)+4, -1);
                    WriteInt(startof(GameObjectInfos)+sizeof(GameObjectInfos)+8, objectCtr - 1);
                } else {
                    for (i=0; i<4; i++)
                        WriteFloat((4*i)+startof(GameObjectInfos)+sizeof(GameObjectInfos), ReadFloat((4*i)+startof(GameObjectInfos)+sizeof(GameObjectInfos))*0.33); //randomize GUID
                    WriteInt(startof(GameObjectInfos)+sizeof(GameObjectInfos) + 16, newId);
                    WriteInt(startof(GameObjectInfos)+sizeof(GameObjectInfos) + 20, -1);
                    WriteShort(startof(GameObjectInfos)+sizeof(GameObjectInfos) + 24, objectCtr - 1);
                    WriteInt(startof(GameObjectInfos)+sizeof(GameObjectInfos) + 28, -1);
                }
                Header.infoCount += 1;
            }
            if (structType == 2) {
                InsertBytes(startof(FolderInfos)+sizeof(FolderInfos), gObjectSz, 0);
                WriteInt(startof(FolderInfos)+sizeof(FolderInfos), newId);
                WriteInt(startof(FolderInfos)+sizeof(FolderInfos) + 4, -1);
                Header.folderCount += 1;
            }
            
            ShowRefreshMessage();
        }
        
