            if (ExposeUserDatas && ReadInt(FTell()+16) && (exists(GameObjectInfos.GameObjectInfo) || exists(BehaviorTree)) && (RSZVersion == "DMC5" || RSZVersion == "RE2")) { //has UserDatas
                realStart = FTell();
