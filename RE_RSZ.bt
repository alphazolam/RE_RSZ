                            FSeek(startof(Test));
                            BLANK blank <bgcolor=cRed>;
                            FSeek(lastGameObject);
                            while (FTell() < FileSize() - 8) {
                                if (ReadUInt(FTell()) == 3212836864 && detectedBools(FTell()-4) 
